{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "additionalProperties": false,
  "description": "Input parameters for searching clinical trial studies.",
  "properties": {
    "city": {
      "description": "Filter studies by city (e.g., "New York", "Toronto"). Searches location/facility fields.",
      "type": "string",
    },
    "conditionQuery": {
      "description": "Condition-specific search  searches only the condition/synonym index. More precise than general query for disease/condition searches (e.g., "Type 2 Diabetes", "non-small cell lung cancer").",
      "type": "string",
    },
    "country": {
      "description": "Filter studies by country (e.g., "United States", "Canada"). Searches location/facility fields.",
      "type": "string",
    },
    "fields": {
      "description": "Specific fields to return (reduces payload size). STRONGLY RECOMMENDED  without this, the full study record (~70KB each) is returned. Example: ["NCTId", "BriefTitle", "OverallStatus", "Phase", "LeadSponsorName", "BriefSummary", "Condition", "InterventionName"]. Use full data only when you need detailed eligibility criteria, locations, or results.",
      "items": {
        "type": "string",
      },
      "type": "array",
    },
    "filter": {
      "description": "Advanced filter expression using AREA[] syntax. Examples: 'AREA[MinimumAge]RANGE[MIN, 18 years]', 'AREA[StudyType]INTERVENTIONAL', 'AREA[Phase](PHASE1 OR PHASE2)'. Combine with AND/OR. Use statusFilter/phaseFilter for simpler status/phase filtering.",
      "type": "string",
    },
    "geoFilter": {
      "description": "Geographic proximity filter. Format: "distance(lat,lon,distanceMiles)"  e.g., "distance(39.0035,-77.1013,50)" for studies within 50 miles of Bethesda, MD.",
      "type": "string",
    },
    "interventionQuery": {
      "description": "Intervention-specific search  searches only intervention/treatment fields (e.g., "pembrolizumab", "cognitive behavioral therapy").",
      "type": "string",
    },
    "pageSize": {
      "default": 10,
      "description": "Number of studies to return per page (1-200). Defaults to 10.",
      "maximum": 200,
      "minimum": 1,
      "type": "integer",
    },
    "pageToken": {
      "description": "Token for retrieving the next page of results.",
      "type": "string",
    },
    "phaseFilter": {
      "anyOf": [
        {
          "description": "A single phase to filter by.",
          "enum": [
            "EARLY_PHASE1",
            "PHASE1",
            "PHASE2",
            "PHASE3",
            "PHASE4",
            "NA",
          ],
          "type": "string",
        },
        {
          "description": "An array of phases to filter by.",
          "items": {
            "enum": [
              "EARLY_PHASE1",
              "PHASE1",
              "PHASE2",
              "PHASE3",
              "PHASE4",
              "NA",
            ],
            "type": "string",
          },
          "minItems": 1,
          "type": "array",
        },
      ],
      "description": "Filter by trial phase. Accepts one or more values: EARLY_PHASE1, PHASE1, PHASE2, PHASE3, PHASE4, NA.",
    },
    "query": {
      "description": "General full-text search query across all fields (conditions, interventions, sponsors, etc.).",
      "type": "string",
    },
    "sort": {
      "description": "Sort order specification. Use valid API field names with optional :asc/:desc suffix (e.g., "LastUpdatePostDate:desc", "EnrollmentCount", "@relevance").",
      "type": "string",
    },
    "sponsorQuery": {
      "description": "Sponsor-specific search  searches the SponsorsModule which includes both lead sponsor and collaborator names (e.g., "Pfizer", "National Cancer Institute"). To filter strictly by lead sponsor, use filter with AREA[LeadSponsorName] instead.",
      "type": "string",
    },
    "state": {
      "description": "Filter studies by state or province (e.g., "California", "Ontario"). Searches location/facility fields.",
      "type": "string",
    },
    "statusFilter": {
      "anyOf": [
        {
          "description": "A single status to filter by.",
          "enum": [
            "ACTIVE_NOT_RECRUITING",
            "COMPLETED",
            "ENROLLING_BY_INVITATION",
            "NOT_YET_RECRUITING",
            "RECRUITING",
            "SUSPENDED",
            "TERMINATED",
            "WITHDRAWN",
            "AVAILABLE",
            "NO_LONGER_AVAILABLE",
            "TEMPORARILY_NOT_AVAILABLE",
            "APPROVED_FOR_MARKETING",
            "WITHHELD",
            "UNKNOWN",
          ],
          "type": "string",
        },
        {
          "description": "An array of statuses to filter by.",
          "items": {
            "enum": [
              "ACTIVE_NOT_RECRUITING",
              "COMPLETED",
              "ENROLLING_BY_INVITATION",
              "NOT_YET_RECRUITING",
              "RECRUITING",
              "SUSPENDED",
              "TERMINATED",
              "WITHDRAWN",
              "AVAILABLE",
              "NO_LONGER_AVAILABLE",
              "TEMPORARILY_NOT_AVAILABLE",
              "APPROVED_FOR_MARKETING",
              "WITHHELD",
              "UNKNOWN",
            ],
            "type": "string",
          },
          "minItems": 1,
          "type": "array",
        },
      ],
      "description": "Filter by overall study status. Accepts one or more values: RECRUITING, COMPLETED, ACTIVE_NOT_RECRUITING, NOT_YET_RECRUITING, etc.",
    },
  },
  "required": [
    "pageSize",
  ],
  "type": "object",
}
